            <th scope="col">Cena_proizvoda(din)</th>
            <th scope="col">Obrisi_proizvod</th>

          </tr>
        </thead>
        <% korpa.forEach(function(kor) { %>
        <tbody>
          <tr>
            
            <td><%=kor.korisnik_id%></td>
            <td><%=kor.proizvod_id%></td>
            <td><%=kor.naziv_proizvoda%></td>
            <td><%=kor.ukupna_cena%></td>
            <form action="/korpa_brisanje/<%= kor.id %>" method="POST">
              <td><button type="submit"><i class="fas fa-trash-alt" style="color: red; border: none;"></i></button></td>
              </form>
          </tr>
          </tbody>
          <%})%>
          
             
          

